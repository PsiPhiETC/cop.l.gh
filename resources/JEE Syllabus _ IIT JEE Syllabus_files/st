<script type="text/javascript">
function NavigateToCs(baseurl,langprefix)
{
	if(langprefix == 'aunz-nz')
		window.location = baseurl + '/aunz/';
	else
		window.location = baseurl + '/' + langprefix + '/';
}
</script>

